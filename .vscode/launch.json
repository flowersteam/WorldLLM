{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Env",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/test_env.py",
            "console": "integratedTerminal",
        },
        {
            "name": "RL",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/test_sb3.py",
            "console": "integratedTerminal",
            "args": [
                "--gamma=0.99",
                "--lr=0.0005",
                "--vf_coef=0.5",
                "--n_steps=256",
                "--n_epochs=4",
                "--gae=0.9"
            ]
        },
        {
            "name": "Playground",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "-cn",
                "playground_metropolis.yaml",
                "algorithm.nb_rules=2",
                "algorithm.nb_iterations=2",
                "algorithm.nb_collecting=2",
                "agent=randomplayground.yaml"
            ]
        }
    ]
}